{
    "list-context.product-list#mobile__desktop--products": {
        "title": "home-product--shelf",
        "blocks": ["product-summary.shelf#desktop__products"],
        "children": ["slider-layout#desktop-products"],
          "props": {
            "blockclass": "desktop__shelf"
          }
      },
      "product-summary.shelf#desktop__products": {
        "title": "home-product--summary",
        "children": [
          //    "stack-layout#prodsum",
          "product-summary-image",
          "product-summary-name",
          "product-summary-price"
      //    "product-summary-sku-selector"
          //         "product-summary-buy-button"
        ]
      },
      "slider-layout#desktop-products": {
        "title": "home-product--slider",
        "props": {
          "itemsPerPage": {
            "desktop": 4,
            "tablet": 2,
            "phone": 2
          },
          "infinite": true,
          "showNavigationArrows": "desktopOnly",
          "blockClass": "products--carousel-desktop",
          "showPaginationDots": "mobileOnly",
          "navigationStep": "page",
          "fullWidth": false
        }
      },
      "rich-text#products__button": {
        "title": "home-product--verTodo",
        "props": {
          "text": "[Ver todo](/)",
          "blockClass": "products--button-desktop"
        }
      }
}