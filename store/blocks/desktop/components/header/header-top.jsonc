        "title": "item-unisex",
        "props": {
            "id": "category-chocolates4",
            "type":"custom",
            "itemProps":{
                "type": "external",
                "href": "/",
                "noFollow":true,
                "tagTitle":"Chocolates",
                "text": "Chocolates unisex"
            }
        }
    },
    "menu-item#category-chocolates--todo": {
        "title": "item-todo",
        "props": {
            "id": "category-chocolates5",
            "type":"custom",
            "itemProps":{
                "type": "external",
                "href": "/",
