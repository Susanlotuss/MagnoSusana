{
    "header-row#header__top": {
        "title": "header__desktop--menu",
        "children": [
            "logo#header__top--logo",
            "menu#web",
            "header-spacer",
            "login",
            "minicart.v2"
        ],
        "props": {
            "sticky": true,
            "blockClass": "desktop__menus"
        }
    },
    "logo#header__top--logo": {
        "title": "Logo",
        "props": {
            "url": "assets/img/magnoLogo.avif",
            "width": 160,
            "height": 68,
            "href": "/"
        }
    },
    "menu#web": {
        "children":[
            "menu-item#chocolates",
            "menu-item#tortas",
            "menu-item#corporativos",
            "menu-item#preguntas",
            "menu-item#blog",
            "menu-item#contactanos"
        ],
        "props":{
          "orientation": "horizontal",
          "blockClass": "container__desktop--menu"
        }
    },
    "menu-item#tortas": {
        "props": {
            "id": "menu-item-tortas",
                "type": "custom",
                "highlight": false,
                "itemProps": {
                  "type": "internal",
                  "href": "/",
                  "noFollow": false,
                  "tagTitle": "tortas",
                  "text": "Tortas"
                }
        }
    },
    "menu-item#corporativos": {
