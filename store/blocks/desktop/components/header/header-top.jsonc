    "menu-item#chocolates": {
        "props": {
            "id": "category-desktop--chocolates",
                "type": "custom",
                "blockClass": "category-submenu",
                "highlight": false,
                "itemProps": {
                  "type": "internal",
                  "href": "#",
                  "noFollow": false,
                  "tagTitle": "Chocolates",
                  "text": "Chocolates"
                }
        },
        "blocks": [
            "submenu.accordion#chocolates"
        ]
    },
    "submenu.accordion#chocolates": {
        "children": [
            "menu#chocolates"
        ],
        "props":{
            "blockClass": "chocolates-container",
            "onMountBehavior": "open"        
          }
    },
