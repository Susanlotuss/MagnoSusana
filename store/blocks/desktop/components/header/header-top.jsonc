                "tagTitle":"Chocolates",
                "text": "Todo"
            }
        }
    },
    "menu-item#category-chocolates--Regalos": {
        "title": "item-regalos",
        "props": {
            "id": "category-chocolates6",
            "type":"custom",
            "itemProps":{
                "type": "external",
                "href": "/",
                "noFollow":true,
                "tagTitle":"Chocolates",
                "text": "Regalos Navidad"
            }
        }
