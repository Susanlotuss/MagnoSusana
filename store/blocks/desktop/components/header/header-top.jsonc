    "modal-trigger#header__top--login": {
        "title": "trigger-login",
        "children": [
            "rich-text#header__top--login",
            "modal-layout#header__top--login"
        ]
    },
    "rich-text#header__top--login": {
        "title": "rich-login",
        "props": {
            "text": "login"
        }
    },
    "modal-layout#header__top--login": {
        "title": "layout-login",
        "children": [
            "login-content"
        ]
    },
    "login#header__desktop": {
        "title": "login-desktop",
        "props": {
            "iconLabel": " ",
            "emailAndPasswordTitle": "Login",
            "accessCodeTitle": "Access Code Login",
            "emailPlaceHolder": "e-mail",
            "passwordPlaceholder": "password",
            "showPasswordVerificationIntoTooltip": true,
            "showIconProfile": true
        }
    },
    "modal-trigger#search__header": {
        "title": "modal-search",
        "children": [
            "rich-text#search",
            "modal-layout#desktop__header"
        ]
    },
    "search-bar#desktop__header": {
        "title": "search-bar--desktop",
        "blocks": ["autocomplete-result-list.v2"],
        "props": {
          "openAutocompleteOnFocus": true,
          "maxWidth": 155
        }
      },
      "modal-layout#desktop__header": {
        "title": "layout-header",
        "children": [
            "search-bar#desktop__header"
        ],
        "props": {
            "fullScreen": false,
            "blockClass": "modal-search"
        }
      },
      "rich-text#search": {
        "title": "rich-search",
        "props": {
            "text": "![search](assets/img/search_magnifier.png)",
            "blockClass": "search-icon"
        }
      },
      "minicart.v2": {
        "props": {
            "itemCountMode": "total",
            "quantityDisplay": "always"
        },
        "children": ["minicart-base-content"]
      },
      "minicart-base-content": {
        "blocks": ["minicart-empty-state"],
        "children": ["minicart-product-list", "flex-layout.row#minicart__footer"]
      },
      "minicart-empty-state": {
        "children": ["flex-layout.row#minicart__empty"]
      },
      "flex-layout.row#minicart__empty": {
        "children": ["icon-cart#minicart__empty--icon", "rich-text#minicart__empty--text"]
      },
      "icon-cart#minicart__empty--icon": {
        "props": {
            "size": 100
        }
      },
      "flex-layout.row#minicart__footer": {
        "children": ["minicart-summary", "minicart-checkout-button"]
      },
      "rich-text#minicart__empty--text": {
        "props": {
            "text": "Tu carrito esta vacío",
            "textAlignment": "CENTER",
            "textPosition": "CENTER"
        }
      }
}