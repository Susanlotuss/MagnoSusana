{
    "header-row#header__top": {
        "title": "header__desktop--menu",
        "children": [
            "logo#header__top--logo",
            "menu#web",
            "header-spacer",
            "modal-trigger#search__header",
    //        "search-bar#desktop__header",
            "login#header__desktop",
            "minicart.v2"
        ],
        "props": {
            "sticky": true,
            "blockClass": "desktop__menus"
        }
    },
    "logo#header__top--logo": {
        "title": "Logo",
        "props": {
            "url": "assets/img/magnoLogo.avif",
            "width": 160,
            "height": 68,
            "href": "/"
        }
    },
    "menu#web": {
        "title": "menu-item--desktop",
        "children":[
            "menu-item#chocolates",
            "menu-item#tortas",
            "menu-item#corporativos",
            "menu-item#preguntas",
            "menu-item#blog",
            "menu-item#contactanos"
        ],
        "props":{
          "orientation": "horizontal",
          "blockClass": "container__desktop--menu"
        }
    },
    "menu-item#tortas": {
        "title": "item-tortas",
        "props": {
            "id": "menu-item-tortas",
                "type": "custom",
                "highlight": false,
                "itemProps": {
                  "type": "internal",
                  "href": "/",
                  "noFollow": false,
                  "tagTitle": "tortas",
                  "text": "Tortas"
                }
        }
    },
    "menu-item#corporativos": {
        "title": "item-corporativo",
        "props": {
            "id": "menu-item-corporativos",
                "type": "custom",
                "highlight": false,
                "itemProps": {
                  "type": "internal",
                  "href": "/",
                  "noFollow": false,
                  "tagTitle": "corporativo",
                  "text": "Corporativos"
                }
        }
    },
    "menu-item#preguntas": {
        "title": "item-preguntas",
        "props": {
            "id": "menu-item-preguntas",
                "type": "custom",
                "highlight": false,
                "itemProps": {
                  "type": "internal",
                  "href": "/",
                  "noFollow": false,
                  "tagTitle": "preguntas",
                  "text": "Preguntas frecuentes"
                }
        }
    },
    "menu-item#blog": {
        "title": "item-blog",
        "props": {
            "id": "menu-item-blog",
                "type": "custom",
                "highlight": false,
                "itemProps": {
                  "type": "internal",
                  "href": "/",
                  "noFollow": false,
                  "tagTitle": "blog",
                  "text": "Blog"
                }
        }
    },
    "menu-item#contactanos": {
        "title": "item-contactanos",
        "props": {
            "id": "menu-item-contactanos",
                "type": "custom",
                "highlight": false,
                "itemProps": {
                  "type": "internal",
                  "href": "/",
                  "noFollow": false,
                  "tagTitle": "contactanos",
                  "text": "Contáctanos"
                }
        }
    },
    "menu-item#chocolates": {
        "title": "item-chocolates",
        "props": {
            "id": "category-desktop--chocolates",
                "type": "custom",
                "blockClass": "category-submenu",
                "highlight": false,
                "itemProps": {
                  "type": "internal",
                  "href": "#",
                  "noFollow": false,
                  "tagTitle": "Chocolates",
                  "text": "Chocolates ˅"
                }
        },
        "blocks": [
            "submenu#chocolates"
        ]
    },
    "submenu#chocolates": {
        "title": "submenu-chocolates",
        "children": [
            "menu#chocolates"
        ],
        "props":{
            "blockClass": "chocolates-container",
            "onMountBehavior": "closed"
          }
    },
    "menu#chocolates": {
        "title": "menu-chocolates",
        "children": [
            "menu-item#category-chocolates--mujer",
            "menu-item#category-chocolates--hombre",
            "menu-item#category-chocolates--sin",
            "menu-item#category-chocolates--unisex",
            "menu-item#category-chocolates--todo",
            "menu-item#category-chocolates--Regalos"
        ],
        "props": {
            "orientation": "vertical",
            "blockClass": "chocolates-list"
        }
    },
    "menu-item#category-chocolates--mujer": {
        "title": "item-mujer",
        "props": {
            "id": "category-chocolates1",
            "type":"custom",
            "itemProps":{
                "type": "external",
                "href": "/",
                "noFollow":true,
                "tagTitle":"Chocolates",
                "text": "Regalos y chocolates para mujer"
            }
        }
    },
    "menu-item#category-chocolates--hombre": {
