  /*row newsletter y social*/
  "flex-layout.row#footer-desktop--newsletter": {
    "children": [
      "flex-layout.col#footer-desktop--newsletters",
      "flex-layout.col#footer-desktop--socialnetwork"
    ],
    "props": {
      "blockClass": "footer__container--letter"
    }
  },
  "flex-layout.col#footer-desktop--newsletters": {
    "title": "newsletters-footer",
    "children": ["newsletter-form#footer__desktop--newsletter"]
    //    "props": {
    //      "blockClass": "footer__container--news"
  },
  "newsletter-form#footer__desktop--newsletter": {
    "children": [
      "rich-text#footer__news--title",
      "newsletter-input-email",
      "newsletter-submit"
    ]
  },
  "newsletter-input-email": {
    "props": {
      "placeholderText": "Correo electrónico"
    }
  },
  "newsletter-submit": {
    "props": {
      "submitButtonLabel": "→"
    }
  },
  "rich-text#footer__news--title": {
    "props": {
      "text": "Suscríbete en nuestro newsletter para recibir promociones y lanzamientos:",
      "blockClass": "news-title"
    }
  },
  "flex-layout.col#footer-desktop--socialnetwork": {
    "title": "socialnetwork-footer",
    "children": ["social-networks"]
  },
  "social-networks": {
    "props": {
      "socialNetworks": [
        { "url": "https://facebook.com/foo", "name": "Facebook" },
        { "url": "https://instagram.com/foo", "name": "Instagram" },
        { "url": "https://tiktok.com/foo", "name": "Tiktok" }
      ],
      "props": {
        "showInColor": true
      }
    }
  },
  "flex-layout.row#footer-desktop--payments": {
    "children": ["flex-layout.col#footer-desktop--payment"],
    "props": {
      "blockClass": "footer__container--payments"
    }
  },
  "flex-layout.col#footer-desktop--payment": {
    "children": ["accepted-payment-methods", "rich-text#footer__copyright"]
  },
  "accepted-payment-methods": {
    "props": {
      "paymentMethods": ["mastercard", "visa", "diners club"],
      "showInColor": true
    }
  },
  "rich-text#footer__copyright": {
    "props": {
      "text": "© 2022, Magno chocolates",
      "blockClass": "footer-copyright"
    }
  }
